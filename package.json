{
  "name": "stencil-components",
  "version": "1.0.0",
  "description": "A stencil component library for PoCs",
  "main": "dist/aepp-components.js",
  "scripts": {
    "@stencil/core": "^0.6.5",
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --config stencil.dev.config.js --dev --watch\" \"stencil-dev-server --config stencil.dev.config.js\"",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "collection": "dist/collection/collection-manifest.json",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@stencil/dev-server": "0.0.18",
    "@stencil/utils": "0.0.4",
    "@types/jest": "^22.1.3",
    "jest": "^22.4.0"
  },
  "types": "dist/types/components.d.ts"
}
